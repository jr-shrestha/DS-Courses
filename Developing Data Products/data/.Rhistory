array_apply = function(X, along, FUN) {
X = as.array(X)
ndim = c(1:length(dim(X)))
preserveAxes = ndim[ndim != along]
orderAxes = c(along, preserveAxes)
X = apply(X, preserveAxes, FUN)
return(aperm(X, orderAxes))
}
